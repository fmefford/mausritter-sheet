{
  "name": "mausritter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 8801",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\"",
    "release": "echo \"Future version will be:\" ; npm run release:local -- --dry-run | grep \"tagging release\" | cut -d\" \" -f4 | cut -c2- ",
    "release:local": "standard-version",
    "release:publish": "npm run release:local && git push --follow-tags"
  },
  "dependencies": {
    "@mdi/font": "^5.9.55",
    "core-js": "^3.6.5",
    "vue": "^3.0.0",
    "vue-i18n": "^9.0.0-rc.2",
    "vuedraggable": "^4.0.1",
    "vuex": "^4.0.0",
    "wave-ui": "^2.12.0"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^2.0.0-beta.4",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "standard-version": "^9.1.1",
    "vue-cli-plugin-i18n": "^2.0.0-beta.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
